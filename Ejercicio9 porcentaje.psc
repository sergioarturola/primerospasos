Algoritmo porcentaje
	Definir num, porcent, resultado Como Real;
	Escribir "Digita el numero que quieres sacar el porcentaje: ";
	Leer num;
	Escribir "Digita el porcentaje %";
	Leer porcent;
	
	resultado<-(num*porcent)/100;
	Escribir"El porcentaje de " num " es " resultado; 
	
FinAlgoritmo
